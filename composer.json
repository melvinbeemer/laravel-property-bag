{
    "name": "zachleigh/laravel-property-bag",
    "description": "Easy Laravel user settings using a property bag",
    "type": "project",
    "keywords": [
        "laravel",
        "user",
        "settings",
        "property",
        "user settings",
        "propety bag"
    ],
    "homepage": "https://github.com/zachleigh/laravel-property-bag",
    "license": "MIT",
    "authors": [
        {
            "name": "Zach Leigh",
            "email": "zachleigh@fastmail.jp"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/cache": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelPropertyBag\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelPropertyBag\\tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "LaravelPropertyBag\\ServiceProvider"
            ]
        }
    }
}